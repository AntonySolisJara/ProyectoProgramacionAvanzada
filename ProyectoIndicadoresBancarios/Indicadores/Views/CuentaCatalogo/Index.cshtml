@model IEnumerable<Indicadores.Models.CuentaCatalogo>

@{
    ViewBag.Title = "Index";
}
<script src="~/Scripts/jquery-3.3.1.min.js"></script>
<script src="~/Scripts/Mantenimientos/FiltroMultiple.js"></script>
<link href="~/Content/Mantenimientos/StyleFiltros.css" rel="stylesheet" />


<h2>Cuentas Obtenidas</h2>
<img src="~/fonts/Imagenes/catalogo.png" />

<input type="text" id="myInput" onkeyup="myFunction()" placeholder="Buscar...">
<table class="table" id="myTable">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IdCtaCatalogo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CtaCatalogo)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody id="myTables">
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.IdCtaCatalogo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CtaCatalogo)
                </td>
            </tr>
        }
    </tbody>
</table>

<br />

<div class="form-group">
    <div class="col-md-offset-2 col-md-10">
        @Html.ActionLink("Siguiente", "Create", "Indicadores")
    </div>
</div>

<br />
